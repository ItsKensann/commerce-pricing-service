{
  "CosmosDb": {
    "DatabaseId": "commercepricing",
    "Containers": [ "commercepricingmaster", "commercepricingtransactions" ],
    "commercePricingTransactionsCollectionName": "commercepricingtransactions",
    "commercepricingMasterCollectionName": "commercepricingmaster",
    "leasesCollectionName": "leases"
  },
  "CosmosResiliencySettings": {
    "MaxRetryCount": 10,
    "MedianFirstRetryDelayMilliseconds": 250
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*"
}
